{
  "name": "@hytalejs.com/core",
  "version": "1.0.0",
  "private": false,
  "description": "TypeScript types and utilities for HytaleJS plugins",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && cp -r dist/** ../../docs/types",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hytale",
    "hytalejs",
    "typescript",
    "types"
  ],
  "author": "bmstefanski",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmstefanski/HytaleJS.git",
    "directory": "packages/core"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
